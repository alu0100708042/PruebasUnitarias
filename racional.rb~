# Implementar en este fichero la clase para crear objetos racionales

require "./gcd.rb"
class Fraccion
	#incializacion del objeto, llamada al m√©todo del constructor
	def initialize (a,b)
		@x=a
		@y=b	
	end
	#convertir a string para mostrarlo 	
        def to_s
                "#{@x}/#{@y}"
        end

end
